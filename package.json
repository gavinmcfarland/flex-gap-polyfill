{
  "name": "flex-gap-polyfill",
  "version": "4.1.0",
  "description": "A PostCSS polyfill for adding gap between flex items",
  "author": "Gavin <someotter@tadpot.com>",
  "license": "CC0-1.0",
  "repository": "limitlessloop/flex-gap-polyfill",
  "homepage": "https://github.com/limitlessloop/flex-gap-polyfill#readme",
  "bugs": "https://github.com/limitlessloop/flex-gap-polyfill/issues",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "files": [
    "dist/index.js",
    "dist/index.js.map",
    "dist/index.mjs",
    "dist/index.mjs.map"
  ],
  "scripts": {
    "build": "rollup --config .rollup.js --silent && node preprocess.js",
    "prepublishOnly": "npm test",
    "pretest:tape": "npm run build",
    "test": "npm run test:js && npm run test:tape && node preprocess.js",
    "test:js": "eslint src/{*,**/*}.js --cache --ignore-path .gitignore --quiet",
    "test:tape": "postcss-tape"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.14.3",
    "@babel/preset-env": "^7.14.4",
    "@rollup/plugin-commonjs": "^19.0.0",
    "autoprefixer": "^10.2.6",
    "babel-eslint": "^10.1.0",
    "chokidar": "^3.5.1",
    "common-tags": "^1.8.0",
    "eslint": "^7.28.0",
    "fs-extra": "^10.0.0",
    "phtml-utility-class": "^3.0.3",
    "postcss": "8.3.6",
    "postcss-load-config": "^3.0.1",
    "postcss-nested": "^5.0.5",
    "postcss-preset-env": "^6.7.0",
    "postcss-tape": "^6.0.1",
    "pre-commit": "1.2.2",
    "rollup": "^2.50.6",
    "rollup-plugin-babel": "^4.4.0",
    "stylup": "^0.0.0-alpha.6",
    "uniqid": "^5.3.0",
    "yarn": "^1.22.10"
  },
  "peerDependencies": {
    "postcss": "^8.3.6"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    },
    "extends": "eslint:recommended",
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2018,
      "impliedStrict": true,
      "sourceType": "module"
    },
    "root": true
  },
  "keywords": [
    "postcss",
    "css",
    "postcss-plugin",
    "gutters",
    "columns",
    "rows",
    "flex",
    "flexbox",
    "polyfill"
  ],
  "dependencies": {
    "postcss-values-parser": "^5.0.0"
  }
}
